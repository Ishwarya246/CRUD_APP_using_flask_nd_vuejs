} )\r\n      this.getDetails();\r\n      // this.list = this.list.filter((email , i) => i != index)\r\n    },\r\n\r\n\r\n    updateDetails(){\r\n      this.getDetails();\r\n      const path = 'http://localhost:5000/update';\r\n      axios.put(path , {\r\n        name : this.editname,\r\n        email : this.editemail,\r\n        age : this.editage,\r\n        rollno : this.editrollno\r\n      })\r\n      .then(response => {\r\n        console.log(response)\r\n      })\r\n      .catch(err => {\r\n        console.log(err)\r\n      })\r\n\r\n      this.list[this.ind].name = this.editname,\r\n      this.list[this.ind].email = this.editemail,\r\n      this.list[this.ind].age = this.editage,\r\n      this.list[this.ind].rollno = this.editrollno,\r\n\r\n      this.ind = \"\",\r\n      this.isEdit = false,\r\n      this.editname = \"\",\r\n      this.editage = \"\",\r\n      this.editemail = \"\",\r\n      this.editrollno = \"\"\r\n\r\n      this.getDetails();\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style>\r\n\r\n.f66{\r\n  color: blue;\r\n  font-family: cursive;\r\n  font-size: 100%;\r\n  font-weight : bold;\r\n  border-radius: 10px;\r\n  background-color: hsl(34, 40%, 90%);\r\n}\r\n\r\n.f55{\r\n  background-color: hsl(34, 40%, 90%);\r\n  width: 20%;\r\n  color: blue;\r\n  font-family: cursive;\r\n  font-size: 100%;\r\n  padding: 2.5px;\r\n  border-color: black;\r\n  margin: 1px;\r\n  font-weight : bold;\r\n}\r\n\r\n.f44{\r\n  display: flex;\r\n  flex-direction: row;\r\n  flex-wrap: wrap;\r\n  background-color: hsl(33, 76%, 71%);\r\n  padding: 5px;\r\n  margin: 2px;\r\n  height: 1cm;\r\n}\r\n\r\n.f33{\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-wrap: wrap;\r\n  margin: 5%;\r\n}\r\n\r\n.button{\r\n  /* display: inline; */\r\n  color :blue;\r\n  font-family: cursive;\r\n  font-size: 20px;\r\n  background-color: antiquewhite;\r\n}\r\n.label{\r\n  font-family: cursive;\r\n  font-size: 20px;\r\n  padding: 2px;\r\n  \r\n}\r\n\r\n.input{\r\n  font-size:20px 